{
    "name": "ucsdmath/pdf",
    "type": "library",
    "description": "A adapter for the mPDF print library.",
    "keywords": ["pdf","ucsdmath", "mpdf", "print", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2017-02-05",
    "authors": [
        {
            "name": "Daryl Eisner",
            "email": "deisner@ucsd.edu"
        }
    ],
    "require": {
        "php": ">=7.0",
        "mpdf/mpdf": "6.1.3",
        "twig/twig": "2.1.0",
        "nesbot/carbon": "1.22.1",
        "twig/extensions": "1.4.1",
        "ucsdmath/functions": "1.13.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.7",
        "phpunit/phpunit": "6.0.5",
        "fzaninotto/faker": "1.6.0",
        "ucsdmath/date": "1.13.0",
        "symfony/var-dumper": "3.2.2",
        "squizlabs/php_codesniffer": "2.8.0"
    },
    "autoload": {
        "psr-4": {
